
 @model LoginPractice.Models.UsersDetail
<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <title>Login Form</title>
    
    <link href="~/LoginFormCSS/style.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href=https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
</head>
<body>

    

    <div class="login-box">

        @using (Html.BeginForm("Autherize", "Login", FormMethod.Post))
        {
            <form>

                <h1>Login</h1>

                <div class="textbox">

                    <i class="fas fa-user"></i>
                    @Html.EditorFor(model => model.UserName,new { style= "placeholder:UserName" })



                </div>

                <div class="textbox">


                    <i class="fas fa-lock-open"></i>

                    @Html.EditorFor(model => model.Password)



                </div>
                <div class="textbox">

                    <input class="btn" type="submit" name="" value="Sign in">


                    <a href="@Url.Action("Register", "Login")">Register Here</a>




                </div>
            </form>

        }

</body>
</html>